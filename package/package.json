{
  "name": "canvaskit-js",
  "version": "0.0.0-development",
  "private": true,
  "description": "A CanvasKit polyfill to the browser APIs",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "tsc": "tsc --noEmit",
    "build": "tsup src/index.ts src/__test__/Utils.ts --format esm,cjs,iife --dts",
    "watch": "tsup src/index.ts src/__test__/Utils.ts --format esm,cjs,iife --dts --watch",
    "lint": "eslint src --ext .ts,.tsx  --max-warnings 0 --cache",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wcandillon/canvaskit-js.git"
  },
  "author": "William Candillon",
  "license": "SEE LICENSE IN LICENSE.md ../License.md",
  "bugs": {
    "url": "https://github.com/wcandillon/canvaskit-js/issues"
  },
  "homepage": "https://github.com/wcandillon/canvaskit-js#readme",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "canvaskit-wasm": "0.38.2",
    "eslint": "^8.41.0",
    "eslint-config-react-native-wcandillon": "^3.10.0",
    "firebase-tools": "^12.6.2",
    "jest": "^29.5.0",
    "jest-diff": "^29.6.0",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0",
    "puppeteer": "^20.4.0",
    "semantic-release": "^22.0.5",
    "ts-jest": "^29.1.0",
    "tsup": "7.1.0"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
