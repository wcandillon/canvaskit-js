{
  "name": "canvaskit-js",
  "version": "1.0.0",
  "description": "A CanvasKit polyfill to the browser APIs",
  "main": "dist/index.js",
  "directories": {
    "example": "example",
    "test": "jest"
  },
  "scripts": {
    "test": "yarn tsc && yarn build && jest --detectOpenHandles",
    "tsc": "tsc --noEmit",
    "build": "tsup src/index.ts src/__test__/Utils.ts --format esm,cjs,iife --dts",
    "watch": "tsup src/index.ts src/__test__/Utils.ts --format esm,cjs,iife --dts --watch",
    "lint": "eslint src --ext .ts,.tsx  --max-warnings 0 --cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wcandillon/canvaskit-lite.git"
  },
  "author": "William Candillon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wcandillon/canvaskit-lite/issues"
  },
  "homepage": "https://github.com/wcandillon/canvaskit-lite#readme",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "canvaskit-wasm": "0.38.2",
    "eslint": "^8.41.0",
    "eslint-config-react-native-wcandillon": "^3.10.0",
    "jest": "^29.5.0",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "puppeteer": "^20.4.0"
  },
  "dependencies": {
    "abs-svg-path": "^0.1.1",
    "normalize-svg-path": "^1.1.0",
    "parse-svg-path": "^0.1.2",
    "serialize-svg-path": "^0.1.0",
    "svg-arc-to-cubic-bezier": "^3.2.0",
    "svg-path-properties": "^1.2.0"
  }
}
